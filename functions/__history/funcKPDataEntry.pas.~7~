unit funcKPDataEntry;

interface

uses dbMod;

  procedure id;

implementation

procedure id();
var
  latestId: Integer;
begin
  with dbModFrm.qTemp do
  begin
    // Get Max id
    SQL.Clear;
    SQL.Text := 'SELECT MAX(id) AS latest_id FROM "Users"';
    Open;

    latestId := FieldByName('latest_id').AsInteger + 1;
  end;
end;

end.
